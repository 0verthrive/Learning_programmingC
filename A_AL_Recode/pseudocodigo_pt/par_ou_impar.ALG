Algoritmo "Par oooou Ímpar"

Var
// Variável
 num : inteiro

Inicio
// Entrada
 Escreval("Digite um valor: ")
 Leia(num)
 
//Processamento e saída
 Se(num %2 = 0) Entao
        EscrevaL("O número", num," é par")
 Senao
        EscrevaL("O número", num," é ímpar")
 Fimse

Fimalgoritmo
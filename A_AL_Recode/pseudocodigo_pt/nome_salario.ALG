Algoritmo "recebendo nome e salário"

Var
// Variável
 nome : caracter
 salario : real
 Inicio
// Entrada
    EscrevaL("Qual seu nome? ")
    Leia(nome)
    EscrevaL("Informe seu salário: ")
    Leia(salario)
    
//Saída
    EscrevaL("")
    EscrevaL("O funcionário ", nome,",recebe o valor salarial: ", salario)



Fimalgoritmo
Algoritmo "somatória"

Var
//Variáveis
   num1, num2, soma : inteiro

Inicio
// Entrada
    EscrevaL("Informe o primeiro número: ")
    Leia(num1)
    EscrevaL("Informe o segundo número: ")
    Leia(num2)
    
// Processamento
    soma <- num1 + num2
    
// Saída
     EscrevaL("A somatória dos valores", num1,"+",num2," é: ", soma)

Fimalgoritmo